<tr>
  <td><%= publisher.name %></td>
  <td><%= publisher.email %></td>
  <td><%= publisher.address %></td>
  <td>
    <%= link_to admin_publisher_path(publisher) do %>
      <i class="fas fa-eye "></i>
    <% end %>
    <span class="mx-2"></span>
    <%= link_to edit_admin_publisher_path(publisher) do %>
      <i class="fas fa-edit"></i>
    <% end %>
    <span class="mx-2"></span>
    <%= link_to  admin_publisher_path(publisher), data: { "turbo-method": :delete,
                                           turbo_confirm: t("admin.delete") } do %>
      <i class="fas fa-trash"></i>
    <% end %>
  </td>
</tr>
